{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "UniverseDb": "YOUR_CONNECTION_STRING"
  },
  "MassTransit": {
    "Username": "YOUR_USERNAME",
    "Password": "YOUR_PASSWORD",
    "Host": "YOUR_HOST"
  },
  "QueueEndpoints": {
    "Universe": "universe-dev",
    "Citizen": "universe-citizen-dev"
  },
  "TickLength": 60,
  "Consul": {
    "Discovery": "YOUR_DISCOVERY_ADDRESS",
    "Host": "YOUR_HOST_ADDRESS",
    "Port": 80,
    "Name": "YOUR_SERVICE_NAME"
  }
}
